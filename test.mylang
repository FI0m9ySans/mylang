# 测试基本功能
打印("你好，世界！");

# 测试字符串和数字的拼接
名字 = "小明";
年龄 = 20;
打印(名字 + "的年龄是" + 年龄);

# 测试字符串重复
分隔线 = "-" * 20;
打印(分隔线);

# 测试算术运算
a = 10;
b = 3;
打印("a + b = " + (a + b));
打印("a - b = " + (a - b));
打印("a * b = " + (a * b));
打印("a / b = " + (a / b));

# 测试比较运算
如果(a > b) {
  打印("a 大于 b");
}

# 测试循环
计数 = 1;
循环(计数 <= 5) {
  打印("计数: " + 计数);
  计数 = 计数 + 1;
}

# 测试错误处理
尝试 {
  打印(未定义变量);  # 这会报错
} 捕获 {
  打印("捕获到错误: " + _error);
}